{
  "name": "stuzzo/monolog-extender",
  "description": "Monolog wrapper that extends formatters and processors to add further informations",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "logging",
    "monolog",
    "processor",
    "formatter"
  ],
  "require": {
    "php": ">=5.5.9",
    "monolog/monolog": "~1.22",
    "symfony/http-foundation": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": {
      "Stuzzo\\Monolog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stuzzo\\Monolog\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.*-dev"
    }
  },
  "authors": [
    {
      "name": "Alfredo -stuzzo- Aiello",
      "email": "stuzzo@gmail.com"
    }
  ],
  "suggest": {
    "stuzzo/monolog-extender-bundle": "Add monolog extender functionalities to Symfony"
  }
}
