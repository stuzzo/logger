{
    "name": "stuzzo/monolog-extender",
    "description": "Monolog wrapper that extends handlers and processors to add further informations",
    "type": "library",
    "license": "MIT",
    "keywords": ["logging", "monolog", "stuzzo", "processor", "formatter"],
    "require": {
        "monolog/monolog": "~1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {"Stuzzo\\Monolog\\": "src/"}
    },
    "autoload-dev": {
    "psr-4": {
        "Stuzzo\\Monolog\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alfredo -stuzzo- Aiello",
            "email": "stuzzo@gmail.com"
        }
    ]
